This is the standard stakeholder template
<p>{{#if isLead}}I am the leader{{else}}I am just a follower{{/if}}</p>
{{#if isLead}}<div>
    <div>Votes remaining: <span id='votes' class='highlight'>{{teamObj.votes}}</span></div>
    <table><tbody>
        {{#each mainTeams}}
        {{#if isMyTeam}}{{else}}
        <tr><td>{{id}}</td><td>{{title}}</td><td><input class='voteVal' id='voteVal_{{id}}' style='width: 40px;' oninput='validateVote(this)' type='number' value='0'><button class='buttonVote' id='buttonVote_{{id}}'>Submit</button></td></tr>
        {{/if}}
        {{/each}}
    </tbody></table>

</div>{{/if}}
<script>
    setupStakeholder();
</script>
